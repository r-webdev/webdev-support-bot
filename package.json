{
  "name": "discord-mdn-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config -x ts-node --files ./src/index.ts",
    "start": "node build/index.js",
    "build": "yarn test:ci && tsc",
    "lint": "eslint src && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint:fix": "eslint --fix src && prettier --write src",
    "lint:types": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mdn/browser-compat-data": "^3.0.3",
    "@sentry/node": "6.9.0",
    "compare-versions": "3.6.0",
    "date-fns": "2.22.1",
    "discord.js": "12.5.3",
    "dom-parser": "0.1.6",
    "html-entities": "1.4.0",
    "mongoose": "5.13.2",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.1",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@sentry/types": "6.9.0",
    "@types/dom-parser": "0.1.1",
    "@types/jest": "26.0.24",
    "@types/mongoose": "5.11.96",
    "@types/node": "14.17.5",
    "@types/node-fetch": "2.5.11",
    "dotenv": "8.6.0",
    "eslint": "7.30.0",
    "eslint-config-galex": "0.9.7",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.11",
    "prettier": "2.3.2",
    "ts-jest": "26.5.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
