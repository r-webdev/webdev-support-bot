{
  "name": "discord-mdn-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "16.13.0 || ^17.0.0"
  },
  "scripts": {
    "dev": "TS_NODE_FILES=true TS_NODE_PROJECT=\"./tsconfig.json\" nodemon -r dotenv/config -x node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "start": "node build/index.js",
    "build": "tsc",
    "lint": "eslint src && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint:fix": "eslint --fix src && prettier --write src",
    "lint:types": "tsc --noEmit",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mdn/browser-compat-data": "4.1.3",
    "@sentry/node": "6.16.1",
    "compare-versions": "4.1.3",
    "date-fns": "2.28.0",
    "discord.js": "13.6.0",
    "dom-parser": "0.1.6",
    "domyno": "1.0.1",
    "fuse.js": "6.5.3",
    "html-entities": "2.3.2",
    "lodash-es": "4.17.21",
    "mongoose": "6.2.1",
    "node-cache": "5.1.2",
    "node-fetch": "3.1.1",
    "node-html-parser": "5.2.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@sentry/types": "6.16.1",
    "@types/dom-parser": "0.1.1",
    "@types/html-entities": "1.3.4",
    "@types/jest": "27.4.0",
    "@types/mongoose": "5.11.97",
    "@types/node": "16.11.22",
    "@types/node-fetch": "3.0.3",
    "dotenv": "10.0.0",
    "eslint": "8.6.0",
    "eslint-config-galex": "3.5.5",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.6",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
